    ns1:hasPropertyTemplate (
      [ a ns1:PropertyTemplate ;
                rdfs:label "has copyright date"@en ;
                ns1:hasLiteralAttributes
            [ a ns1:LiteralPropertyTemplate ;
                        ns1:hasDefault "2020"@en ;
