    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    targetNamespace="https://uwlib-cams.github.io/sinopia_maps/xsd/" elementFormDefault="qualified"
    vc:minVersion="1.1">

    <xs:simpleType name="format_type">
        <xs:annotation>
            <xs:documentation>
                <!-- to do -->
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <!-- standalone/nested -->
            <xs:enumeration value="standalone"/>
            <xs:enumeration value="nested"/>
            <!-- resource formats -->
            <!-- collection aggregates and parallel aggregates -->
            <xs:enumeration value="aggregate:printMonograph"/>
            <!-- augmenting aggregates and non-aggregates -->
            <xs:enumeration value="printMonograph"/>
            <!-- ... -->
            <!-- if format is not applicable -->
            <xs:enumeration value="na"/>
            <!-- for testing -->
            <xs:enumeration value="test"/>
        </xs:restriction>
    </xs:simpleType>

</xs:schema>
