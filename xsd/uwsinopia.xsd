         <xs:enumeration value="UWSINOPIA:WAU:rdaNomen:identifier:CAMS"/>
         <xs:enumeration value="UWSINOPIA:WAU:rdaEntity:test:ries07"/>
         <xs:enumeration value="UWSINOPIA:WAU:rdaEntity:standalone:ries07"/>
         <xs:enumeration value="UWSINOPIA:WAU:rdaWork:test:ries07"/>
         <xs:enumeration value="UWSINOPIA:WAU:rdaWork:test:cspayne"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:complexType name="p_type" mixed="true">
      <xs:all>
         <xs:element name="strong" type="xs:token" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element ref="a" minOccurs="0" maxOccurs="unbounded"/>
      </xs:all>
   </xs:complexType>
   <xs:simpleType name="simple_p_type">
      <xs:restriction base="xs:string"/>
   </xs:simpleType>
   <xs:element name="a">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute name="href" type="xs:anyURI" use="required"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="ul">
      <xs:complexType>
         <xs:all>
            <xs:element name="li" type="li" minOccurs="1" maxOccurs="unbounded"/>
         </xs:all>
      </xs:complexType>
   </xs:element>
   <xs:element name="ol">
      <xs:complexType>
         <xs:choice>
            <xs:element name="li" type="li" minOccurs="1" maxOccurs="unbounded"/>
         </xs:choice>
      </xs:complexType>
   </xs:element>
