      <xs:all>
         <xs:element name="strong" type="xs:token" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element ref="a" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element ref="ul" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element ref="ol" minOccurs="0" maxOccurs="unbounded"/>
      </xs:all>
   </xs:complexType>
   <!-- *************** implementation_set *************** -->
   <xs:complexType name="implementation_set_type">
      <xs:annotation>
         <xs:documentation>Each implementation set can be used to store a single set of details for
            using the given property in Sinopia.</xs:documentation>
         <xs:documentation>These details can include specifications for whether the property
            template should prompt entry of a literal vs. an IRI, whether it is repeatable, and so
            forth.</xs:documentation>
         <xs:documentation>Each such setting is recorded in an element within the implementation
            set.</xs:documentation>
         <xs:documentation>The same implementation set--that is, the same details for implementing a
            property--can be used in multiple resource templates.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <!-- REQUIRED: institution / resource / format / user -->
         <xs:element name="institution" type="institution_type" minOccurs="1" maxOccurs="unbounded"/>
         <xs:element name="resource" type="resource_label_type" minOccurs="1" maxOccurs="unbounded"/>
         <xs:element name="format" type="format_type" minOccurs="1" maxOccurs="unbounded"/>
         <xs:element name="user" type="user_type" minOccurs="1" maxOccurs="unbounded"/>
         <xs:element name="form_order" type="form_order_type" minOccurs="1" maxOccurs="1"/>
         <!-- OPTIONAL: alt_pt_label / multiple_prop / remark_url / remark / 
            language_suppressed / required / repeatable -->
         <xs:element name="alt_pt_label" type="uwmaps:lang_string_type" minOccurs="0" maxOccurs="1"/>
         <xs:element name="multiple_prop" type="multiple_prop_type" minOccurs="0" maxOccurs="1"/>
         <xs:element name="remark_url" type="uwmaps:iri_type" minOccurs="0" maxOccurs="1"/>
         <xs:element name="remark" type="uwmaps:lang_string_type" minOccurs="0" maxOccurs="1"/>
         <!-- if language_suppressed == 1 *and* default literal has @xml:lang will anything break?? -->
         <xs:element name="language_suppressed" type="xs:boolean" minOccurs="0" maxOccurs="1"
            default="0">
            <xs:annotation>
               <xs:documentation>To indicate that any value provided for a given property template
                  will not have linguistic content, enter the `language_suppressed` element with
                  value 'true' or '1'.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="required" type="xs:boolean" minOccurs="0" maxOccurs="1" default="0">
            <xs:annotation>
               <xs:documentation>To indicate that a value must be provided for a given property
                  template, enter the `required` element with value 'true' or
                  '1'.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="repeatable" type="repeatable_type" minOccurs="0" maxOccurs="1"
            default="0"/>
         <!-- REQUIRED: EITHER literal_pt / uri_pt / lookup_pt / nested_resource_pt (+ subelements) -->
         <xs:choice>
            <xs:element name="literal_pt" type="literal_attributes_type"/>
            <xs:element name="uri_pt" type="uri_attributes_type"/>
            <xs:element name="lookup_pt" type="lookup_attributes_type"/>
            <xs:element name="nested_resource_pt" type="nested_resource_attributes_type"/>
         </xs:choice>
      </xs:sequence>
      <xs:attribute name="localid_implementation_set" type="uwmaps:localid_attr" use="required"/>
   </xs:complexType>
   <!-- form_order_type -->
   <xs:simpleType name="form_order_type">
      <xs:annotation>
         <xs:documentation>The form_order type value determines where this property template will
            appear in the resource template.</xs:documentation>
         <xs:documentation>Enter a decimal value between 0.0 and 1.0--any number of decimal places
            may be used.</xs:documentation>
         <xs:documentation>Property templates with *lower* form_order values will appear before
            those with *higher* values.</xs:documentation>
         <xs:documentation>For example, property templates (by form_order value) would appear in the
            following order: 0.001, 0.01, 0.5.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:decimal">
         <xs:minInclusive value="0.0"/>
         <xs:maxInclusive value="1.0"/>
      </xs:restriction>
   </xs:simpleType>
   <!-- multiple_prop element / subelements -->
   <xs:complexType name="multiple_prop_type">
      <xs:annotation>
         <xs:documentation>Enter the 'multiple_prop' element to create a drop-down property
            selection in the property template.</xs:documentation>
         <xs:documentation>When using the resource template for description, this will allow users
            to select from among the drop-down properties for use.</xs:documentation>
         <xs:documentation>Do not enter any text content in this element; use *either* the
            `all_subprops` subelement, or one or more `property_selection` subelements to specify
            which properties should be made available in the property template.</xs:documentation>
         <xs:documentation>To include all subproperties of the 'prop' element property in the
            drop-down, enter the `multiple_prop` &gt; 'all_subprops' child element with text content
            'true'.</xs:documentation>
         <xs:documentation>To select specific properties to include in the drop-down, use one or
            more 'property_selection' elements (as many as needed) to provide property IRIs and
            labels.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:choice>
            <xs:element name="property_selection" type="property_selection_type" minOccurs="1"
               maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation>Use the property_selection element to enter a specific list of
                     properties to include as options in the property template.</xs:documentation>
                  <xs:documentation>Include a property IRI, and language tag as attribute values,
                     and a property label as child text.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="all_subprops" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
            <xs:element name="all_all_subprops" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
         </xs:choice>
      </xs:sequence>
   </xs:complexType>
   <xs:simpleType name="property_label">
      <xs:restriction base="xs:string">
         <xs:pattern value=".+"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:complexType name="property_selection_type">
      <xs:annotation>
         <xs:documentation>Enter the full, non-prefixed property IRI as the content of the
            'property_iri' attribute.</xs:documentation>
         <xs:documentation>Enter a property label as the text content of the 'property'
            element.</xs:documentation>
      </xs:annotation>
      <xs:simpleContent>
         <xs:extension base="property_label">
            <xs:attribute name="property_iri" type="xs:anyURI" use="required"/>
            <xs:attribute ref="xml:lang" use="required"/>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
   <!-- repeatable_type -->
   <xs:complexType name="repeatable_type">
      <xs:annotation>
         <xs:documentation>To indicate that a multiple values may be entered for a given property
            template, enter the `repeatable` element with value 'true' or '1'.</xs:documentation>
      </xs:annotation>
      <xs:simpleContent>
         <xs:extension base="xs:boolean">
            <xs:attribute name="ordered" type="xs:boolean">
               <xs:annotation>
                  <xs:documentation>To indicate that the order of multiple values for a repeatable
                     property should be preserved, use the `ordered` attribute with value 'true' or
                     '1'.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
