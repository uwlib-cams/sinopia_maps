   <!-- **************** sinopia **************** -->
   <xs:complexType name="sinopia_type">
      <xs:sequence>
         <xs:element name="implementation_set" type="implementation_set_type" minOccurs="1"
            maxOccurs="unbounded"/>
      </xs:sequence>
   </xs:complexType>
   <!-- *************** implementation_set *************** -->
   <xs:complexType name="implementation_set_type">
      <xs:annotation>
         <xs:documentation>Each implementation set can be used to store a single set of details for
            using the given property in Sinopia.</xs:documentation>
         <xs:documentation>These details can include specifications for whether the property
            template should prompt entry of a literal vs. an IRI, whether it is repeatable, and so
            forth.</xs:documentation>
         <xs:documentation>Each such setting is recorded in an element within the implementation
            set.</xs:documentation>
         <xs:documentation>The same implementation set--that is, the same details for implementing a
            property--can be used in multiple resource templates.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <!-- all RTs must have ID components: resource / format / user -->
         <xs:element name="resource" type="resource_label_type" minOccurs="1" maxOccurs="unbounded"/>
         <xs:element name="format" type="format_type" minOccurs="1" maxOccurs="unbounded"/>
         <xs:element name="user" type="user_type" minOccurs="1" maxOccurs="unbounded"/>
         <!-- All implementation_sets must include form_order -->
         <xs:element name="form_order" type="form_order_type" minOccurs="1" maxOccurs="1"/>
         <!-- optional: multiple_prop / remark_url / remark / language_suppressed / required / repeatable -->
         <xs:element name="multiple_prop" type="multiple_prop_type" minOccurs="0" maxOccurs="1"/>
         <xs:element name="remark_url" type="uwmaps:iri_type" minOccurs="0" maxOccurs="1"/>
         <xs:element name="remark" type="uwmaps:lang_string_type" minOccurs="0" maxOccurs="1"/>
         <!-- if language_suppressed == 1 *and* default literal has @xml:lang will anything break?? -->
         <xs:element name="language_suppressed" type="xs:boolean" minOccurs="0" maxOccurs="1"
            default="0"/>
         <xs:element name="required" type="xs:boolean" minOccurs="0" maxOccurs="1" default="0"/>
         <xs:element name="repeatable" type="repeatable_type" minOccurs="0" maxOccurs="1"
            default="0"/>
         <xs:choice>
            <xs:element name="literal_pt" type="literal_attributes_type"/>
            <xs:element name="uri_pt" type="uri_attributes_type"/>
            <xs:element name="lookup_pt" type="lookup_attributes_type"/>
            <xs:element name="nested_resource_pt" type="nested_resource_attributes_type"/>
         </xs:choice>
      </xs:sequence>
      <xs:attribute name="localid_implementation_set" type="uwmaps:localid_attr" use="required"/>
   </xs:complexType>
   <!-- form_order_type -->
   <xs:simpleType name="form_order_type">
      <xs:restriction base="xs:decimal">
         <xs:minInclusive value="0.0"/>
         <xs:maxInclusive value="1.0"/>
      </xs:restriction>
   </xs:simpleType>
   <!-- multiple_prop element / subelements -->
   <xs:complexType name="multiple_prop_type">
      <xs:annotation>
         <xs:documentation>The 'multiple_prop' element is used to create a drop-down property list
            in the property template.</xs:documentation>
         <xs:documentation>When using the resource template, users can select from among the
            drop-down properties for use.</xs:documentation>
         <xs:documentation>To include all subproperties of the 'prop' element property in the
            drop-down, use the 'multiple_prop' &gt; 'all_subprops' child element, and enter the
            value 'true'.</xs:documentation>
         <xs:documentation>To select properties to include in the drop-down, use 'property' elements
            (as many as needed) to provide property IRIs and labels.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:choice>
            <xs:element name="all_subprops" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
            <xs:element name="property" type="property_selection_type" minOccurs="1"
               maxOccurs="unbounded"/>
         </xs:choice>
      </xs:sequence>
   </xs:complexType>
   <!-- to do: constrain further, require text element content -->
   <xs:complexType name="property_selection_type">
      <xs:annotation>
         <xs:documentation>Enter a property label as the text content of the 'property'
            element.</xs:documentation>
         <xs:documentation>Enter the full, non-prefixed property IRI as the content of the
            'property_iri' attribute.</xs:documentation>
      </xs:annotation>
      <xs:simpleContent>
         <xs:extension base="xs:string">
            <xs:attribute name="property_iri" type="xs:anyURI" use="required"/>
            <xs:attribute ref="xml:lang" use="required"/>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
   <!-- repeatable_type -->
   <xs:complexType name="repeatable_type">
      <xs:simpleContent>
         <xs:extension base="xs:string">
            <xs:attribute name="ordered" type="xs:boolean"/>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
